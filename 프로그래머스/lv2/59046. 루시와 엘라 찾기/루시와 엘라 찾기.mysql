SELECT ANIMAL_INS.ANIMAL_ID as ANIMAL_ID, ANIMAL_INS.NAME as NAME, ANIMAL_INS.SEX_UPON_INTAKE as SEX_UPON_INTAKE FROM ANIMAL_INS WHERE ANIMAL_INS.NAME = 'Ella' or ANIMAL_INS.NAME = 'Lucy' or ANIMAL_INS.NAME = 'Pickle' or ANIMAL_INS.NAME = 'Rogan' or ANIMAL_INS.NAME = 'Sabrina' or ANIMAL_INS.NAME = 'Mitty';